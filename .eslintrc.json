{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],

  // React available globally
  "globals": {
    "React": "readonly"
  },

  // Pre-fix unused vars
  "rules": {
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
